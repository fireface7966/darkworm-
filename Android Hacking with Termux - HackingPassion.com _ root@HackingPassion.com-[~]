.yrbl__wrap .section__actions, .yrbl__wrap .claim-preview__primary-actions, .yrbl__wrap .section__actions--centered, .yrbl__wrap .section__actions--between, .yrbl__wrap .section__actions--no-margin { flex-wrap: wrap; }

[dir] .yrbl__wrap .section__actions > *, [dir] .yrbl__wrap .claim-preview__primary-actions > *, [dir] .yrbl__wrap .section__actions--centered > *, [dir] .yrbl__wrap .section__actions--between > *, [dir] .yrbl__wrap .section__actions--no-margin > * { margin-bottom: var(--spacing-s); }

@media (min-width: 900px) {
  .yrbl__wrap { flex-direction: row; }
}

.yrbl { display: none; height: 10rem; }

@media (min-width: 900px) {
  .yrbl { display: block; height: 20rem; }
  [dir="ltr"] .yrbl { margin-right: calc(var(--spacing-xl) * 2); }
  [dir="rtl"] .yrbl { margin-left: calc(var(--spacing-xl) * 2); }
}

.yrbl--always-show { display: block; }

.yrbl--small { display: none; }

@media (min-width: 900px) {
  .yrbl--small { display: block; height: 8rem; }
  [dir="ltr"] .yrbl--small { margin-right: calc(var(--spacing-xl)); }
  [dir="rtl"] .yrbl--small { margin-left: calc(var(--spacing-xl)); }
}

.yrbl__content { max-width: 400px; }

.yrbl--first-run { align-self: center; height: 250px; width: auto; }

[dir] .yrbl--first-run { margin: 0 var(--spacing-l); }

.yrbl--search { justify-content: flex-start; }

[dir] .yrbl--search { margin: 0px; }

.yrbl--search .yrbl { height: 18rem; }

.yrbl--enhanced { position: absolute; z-index: 9999; height: 95vh; width: 95vh; opacity: 0.5; }

[dir="ltr"] .yrbl--enhanced { left: 0px; right: 0px; animation-name: enhancedAnimation-ltr; animation-duration: 2s; animation-iteration-count: infinite; animation-direction: alternate; animation-timing-function: ease-out; }

[dir="rtl"] .yrbl--enhanced { right: 0px; left: 0px; animation-name: enhancedAnimation-rtl; animation-duration: 2s; animation-iteration-count: infinite; animation-direction: alternate; animation-timing-function: ease-out; }

@-webkit-keyframes enhancedAnimation-ltr { 
  0% { left: 0px; transform: rotate(0deg); }
  100% { left: 50vw; transform: rotate(360deg); }
}

@-webkit-keyframes enhancedAnimation-rtl { 
  0% { left: 0px; transform: rotate(0deg); }
  100% { left: 50vw; transform: rotate(-360deg); }
}

.empty__wrap { display: flex; align-items: center; flex-flow: column wrap; justify-content: center; }

[dir="ltr"] .empty__wrap { text-align: left; }

[dir="rtl"] .empty__wrap { text-align: right; }

@media (min-width: 900px) {
  .empty__wrap { flex-direction: row; }
}

[dir] .empty__wrap--padded { padding: var(--spacing-xl) 0; }

.empty__text { color: var(--color-text-empty); font-style: italic; }

.empty__content { max-width: 400px; }
